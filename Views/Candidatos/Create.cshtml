@model crudmysql.Models.Candidatos



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>Candidatos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Nome" class="control-label"></label>
                <input asp-for="Nome" class="form-control" />
                <span asp-validation-for="Nome" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Cpf" class="control-label"></label>
                <input asp-for="Cpf" class="form-control" />
                <span asp-validation-for="Cpf" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Valorp" class="control-label"></label>
                <input asp-for="Valorp" class="form-control" />
                <span asp-validation-for="Valorp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Situacao" class="control-label"></label>
                <input asp-for="Situacao" class="form-control" />
                <span asp-validation-for="Situacao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Numurna" class="control-label"></label>
                <input asp-for="Numurna" class="form-control" />
                <span asp-validation-for="Numurna" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ocupacao" class="control-label"></label>
                <input asp-for="Ocupacao" class="form-control" />
                <span asp-validation-for="Ocupacao" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Vice" /> @Html.DisplayNameFor(model => model.Vice)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Ver" /> @Html.DisplayNameFor(model => model.Ver)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="Partido" class="control-label"></label>
                <input asp-for="Partido" class="form-control" />
                <span asp-validation-for="Partido" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VicePref" class="control-label"></label>
                <input asp-for="VicePref" class="form-control" />
                <span asp-validation-for="VicePref" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Datanasc" class="control-label"></label>
                <input asp-for="Datanasc" class="form-control" />
                <span asp-validation-for="Datanasc" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
